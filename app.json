{
  "expo": {
    "name": "Bible-KJV Biblia-RV",
    "slug": "bible-react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bible512.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/bible512.png",
      "resizeMode": "cover",
      "backgroundColor": "#7dfcd2"
    },
    "newArchEnabled": true,
    "androidNavigationBar": {
      "visible": "sticky-immersive"
    },
    "backgroundColor": "#000000",
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/bible3.png",
        "backgroundColor": "#7dfcd2"
      },
      "package": "com.elorat.biblereactnative",
      "versionCode": 2
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/bible512.png",
          "imageWidth": 150,
          "resizeMode": "cover",
          "backgroundColor": "#7dfcd2"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7d53b729-8c36-40d3-ac05-bf29bd4a393c"
      }
    }
  }
}
